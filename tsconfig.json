{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "target": "ESNext",
    "lib": ["ESNext"],
    "module": "ESNext",
    "sourceMap": false,
    "strict": true,
    "removeComments": false,
    "noEmitOnError": false,
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
